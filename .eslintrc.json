{
    "extends": "airbnb-base",
    "rules": {
        "arrow-parens": "off",
        "import/prefer-default-export": "off",
        "no-unused-vars": [
            2,
            {
                "vars": "all",
                "args": "after-used",
                "ignoreRestSiblings": true,
                "argsIgnorePattern": "^_"
            }
        ]
    }
}